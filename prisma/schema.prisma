datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id           Int    @id @default(autoincrement())
  NIK          String @unique
  nama_lengkap String
  email        String @unique
  username     String @unique
  password     String
}
