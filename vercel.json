{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use":"@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest":"/"
        }
    ],
    "hooks": {
        "build": {
            "command": "npm run build"
        }

    }
}